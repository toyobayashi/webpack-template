{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:9090",
      "port": 9222,
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/dist",
      "sourceMapPathOverrides": {
        "webpack:///*": "${workspaceFolder}/*",
        "webpack:///./*": "${workspaceFolder}/*"
      }
    }
  ]
}
