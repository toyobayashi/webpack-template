{
  "compilerOptions": {
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "es6"
  },
  "include": [
    "./src/**/*"
  ]
}
