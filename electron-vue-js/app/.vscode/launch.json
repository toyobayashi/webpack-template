{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "main",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
      },
      "runtimeArgs": [
        "${workspaceRoot}"
      ],
      "sourceMaps": true,
      "protocol": "inspector"
    }
  ]
}
